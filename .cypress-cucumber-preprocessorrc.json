{
    "json": {
        "enabled": false,
        "output": "./jsonlogs/log.json",
        "formatter": "cucumber-json-formatter.exe"
    },
    "messages": {
        "enabled": false,
        "output": "./jsonlogs/messages.ndjson"
    },
    "html": {
        "enabled": false,
        "html.output": "./cypress/reports"
    },
    "stepDefinitions": [
        "[filepath]/**/*.{js,ts}",
        "[filepath].{js,ts}",
        "cypress/e2e/*.{js,ts}",
        "[filepath]/***.{js,ts}",
        "[filepath].{js,ts}",
        "cypress/e2e/*.js"
    ]
}